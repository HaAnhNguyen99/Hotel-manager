{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "Booking",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "checkin": {
      "type": "datetime"
    },
    "checkout": {
      "type": "datetime"
    },
    "prepayment": {
      "type": "biginteger"
    },
    "reduction": {
      "type": "biginteger"
    },
    "booking_date": {
      "type": "datetime"
    },
    "room": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::room.room",
      "inversedBy": "bookings"
    },
    "guest_name": {
      "type": "string",
      "default": "Vô danh"
    },
    "service_usages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service-usage.service-usage",
      "mappedBy": "booking"
    },
    "cccd": {
      "type": "string",
      "regex": "^\\d+$"
    },
    "booking_status": {
      "type": "enumeration",
      "enum": [
        "Confirmed",
        "Canceled",
        "Completed"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Giờ",
        "Qua Đêm",
        "Ngày"
      ]
    }
  }
}
