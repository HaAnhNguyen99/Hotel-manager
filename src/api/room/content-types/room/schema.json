{
  "kind": "collectionType",
  "collectionName": "rooms",
  "info": {
    "singularName": "room",
    "pluralName": "rooms",
    "displayName": "room",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "room_number": {
      "type": "string"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "room_status": {
      "type": "enumeration",
      "enum": [
        "Available",
        "Cleaning",
        "Occupied"
      ],
      "default": "Available"
    },
    "payments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::reservation.reservation",
      "mappedBy": "rooms"
    },
    "service_usages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service-usage.service-usage",
      "mappedBy": "rooms"
    },
    "floor": {
      "type": "integer"
    },
    "note": {
      "type": "richtext"
    },
    "price_per_night": {
      "type": "biginteger"
    },
    "first_hourly_price": {
      "type": "biginteger"
    },
    "after_hour_price": {
      "type": "biginteger"
    }
  }
}
